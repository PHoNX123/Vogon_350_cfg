[server]
host = 0.0.0.0
port = 7125
klippy_uds_address = /home/phonx/printer_data/comms/klippy.sock

[octoprint_compat]

[history]

[update_manager]
channel = dev
refresh_interval = 168

[update_manager mainsail]
type = web
channel = stable
repo = mainsail-crew/mainsail
path = ~/mainsail

[file_manager]
enable_object_processing = True

[update_manager pgcode]
type = git_repo
primary_branch = main
origin = https://github.com/Kragrathea/pgcode.git
path = ~/pgcode
is_system_service = False

